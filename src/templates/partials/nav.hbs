<div class="stripe stripe--collection {{#if_eq navStyle 'dark'}}stripe--inverted inverted{{/if_eq}}">

	<div class="section border--none">
		<div class="flex flex--wrap flex--column atMedium_flex--row flex--alignCenter flex--spaceBetween">

			<div class="flex-item flex-item--shrink flex--spaceBetween span--100 _docs__atMedium_span--auto">
				<div class="flex flex--alignCenter">
					<div class="flex-item flex-item--shrink display--block atMedium_display--none">
						<div class="chunk js_hamburger"><span>â˜°</span></div>
					</div>
					<div class="flex-item flush--left">
						<div class="flex flex--alignCenter atMedium_flex--rowReverse">
							{{#unless_eq navStyle 'dark'}}
								<div class="flex-item">
									<div class="chunk">
										<h1 class="text--pageTitle">
											<a href="/">M<span class="display--none atMedium_display--inline">eetup</span> D<span class="display--none atMedium_display--inline">esign</span> S<span class="display--none atMedium_display--inline">ystem</span></a>
										</h1>
									</div>
								</div>
							{{/unless_eq}}
							<div class="flex-item flex-item--shrink __docs_flexMarginAlign--left">
								<div class="chunk"><a href="/"><img src="/assets/swarmLogo.svg" /></a></div>
							</div>
						</div>
					</div>
				</div>
			</div>

			<nav class="js_mainNav flex-item flex-item--shrink flush--left display--none atMedium_display--block span--100 _docs__atMedium_span--auto">
				<div class="chunk">
					<ul class="tabs tabs--noBorder flex flex--column atMedium_flex--row flex--noGutters">
						{{#each collections}}
							{{#unless_eq @key 'Resources'}}
								<li class="flex-item tabs-tab margin--none __docs_atMedium_margin--left {{#if_eq @key (arrToStr ../collection)}}tabs-tab--selected{{/if_eq}}"><a href="/{{this.0.path}}">{{@key}}</a></li>
							{{/unless_eq}}
						{{/each}}
					</ul>
				</div>
			</nav>

		</div>
	</div>
</div>
