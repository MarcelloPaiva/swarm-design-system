<div class="stripe">
	<div class="section">
		<div class="flex flex--alignCenter">
			<div class="flex-item flex-item--shrink">
				<div class="chunk"><span>logo</span></div>
			</div>

			<div class="flex-item">
				<div class="chunk">
					<h1 class="text--pageTitle">Swarm Design System</h1>
				</div>
			</div>

			<nav class="flex-item flex-item--shrink">
				<div class="chunk">
					<ul class="tabs">
						{{#each collections}}
							<li class="tabs-tab align--center {{#if_eq @key (arrToStr ../collection)}}tabs-tab--selected{{/if_eq}}"><a href="/{{this.0.path}}">{{@key}}</a></li>
						{{/each}}
					</ul>
				</div>
			</nav>

		</div>
	</div>
</div>
